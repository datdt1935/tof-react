    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.7",
    "@types/react-dom": "^16.9.12",
    "@types/react-redux": "^7.1.16",
    "classnames": "^2.3.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "^2.0.0",
    "express": "^4.17.1",
    "fabric": "^4.3.1",
    "history": "^4.10.1",
    "immer": "^8.0.4",
    "lodash": "^4.17.21",
    "mqtt": "^4.2.6",
    "multicast-dns": "^6.2.3",
    "network-list": "^1.1.5",
    "node-pre-gyp": "^0.15.0",
    "notistack": "^1.0.6",
    "react": "^17.0.2",
    "react-addons-update": "^15.6.3",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "react-scripts": "4.0.3",
    "react-table": "^7.6.3",
    "redux-devtools-extension": "^2.13.9",
    "swagger_tof_pack": "file:gen/api",
    "typescript": "^4.1.5"
  },
  "author": "Xoontec devs <xoontec@devs.com>",
  "description": "Tag",
  "scripts": {
    "dev": "nf start",
    "startdev": "cross-env BROWSER=none react-scripts start",
    "electron": "electron .",
    "dev1": "concurrently  \"npm start\" \"node src/electron-wait-react\" ",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:5000 && electron .",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm electron:start\"",
    "electron:build": "react-scripts build && electron-builder -c.extraMetadata.main=build/electron.js",
    "electron:build-mac": "react-scripts build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:build-linux": "react-scripts build && electron-builder -l -c.extraMetadata.main=build/electron.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "electron-is-dev": "^2.0.0",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "api": "rm -rf gen/api && openapi-generator-cli generate --input-spec server/swagger.json --generator-name typescript-axios --output gen/api --config api.json && cd gen/api && npm install && npm run build"
  },
  "homepage": ".",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@openapitools/openapi-generator-cli": "^2.2.5",
    "@types/fabric": "^4.2.5",
    "@types/lodash": "^4.14.168",
    "@types/react": "^16.14.5",
    "@types/react-addons-update": "^0.14.20",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-router-redux": "^5.0.18",
    "@types/react-table": "^7.0.29",
    "@types/react-virtualized": "^9.21.11",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "^2.0.0",
    "foreman": "^3.0.1",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "wait-on": "^5.3.0"
  },
  "build": {
    "extends": null,
    "appId": "com.example.electron-cra",
    "npmRebuild": false,
    "files": [
      "build/**/*"
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  }
}